{
    "name": "sunnysideac/theme",
    "description": "Sunnyside AC WordPress Theme",
    "type": "wordpress-theme",
    "require": {
        "php": ">=7.4",
        "filp/whoops": "^2.15",
        "posthog/posthog-php": "^3.7",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "SunnysideAC\\": "inc/"
        },
        "classmap": [
            "inc/performance-monitor.php",
            "inc/class-service-city-sitemap-provider.php",
            "inc/navigation.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}

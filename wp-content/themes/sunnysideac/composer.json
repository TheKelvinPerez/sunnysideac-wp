{
    "name": "sunnysideac/theme",
    "description": "Sunnyside AC WordPress Theme",
    "type": "wordpress-theme",
    "require": {
        "php": ">=7.4",
        "filp/whoops": "^2.15",
        "timber/timber": "^2.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "SunnysideAC\\": "inc/"
        }
